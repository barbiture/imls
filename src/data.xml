        </repr:Style>

        <repr:Template gml:id="balloonTemplate">
            <repr:text><![CDATA[
                    <div style="float: left; width:135px; margin: 10px 0 10px 10px;">
                        <img src="http://$[metaDataProperty.AnyMetaData.pic|не задан]" alt="" style="width: 100%" />
                        <span class="icon $[metaDataProperty.AnyMetaData.item| ]"></span>
                        <span class="icon $[metaDataProperty.AnyMetaData.item2| ]"></span>
                    </div>
                    <div style="font-size:12px; font-family: Open Sans; line-height: 22px; color: #fff; margin: 10px 0 10px 155px;">
                        <div style="font-size:14px; font-weight:800; margin: 5px 0">$[name]</div>
                        <div>$[metaDataProperty.AnyMetaData.address]</div>
                        <div>Категория: $[metaDataProperty.AnyMetaData.category|не задан]</div>
                    </div>]]></repr:text>
        </repr:Template>
    </repr:Representation>

