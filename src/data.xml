    <repr:Representation>
        <repr:Style gml:id="2">
            <repr:iconStyle>
                <repr:href>http://pagecreate.ru/rcevo/images/2.png</repr:href>
                <repr:size x="42" y="52"/>
                <repr:offset x="-19" y="-15"/>
